<swf>
	<Header/>
	<FileAttributes/>
	
	<DefineABC name="Main">
		<abcfiles>
		<abcfile>
			<import class="flash.display.MovieClip" />

			<class name="Main" extends="MovieClip">
				
				<var name="str1"  type="String" value="hello world" />
				<var name="arr1"  type="Array" />
				
				<var static="true" name="str2" type="String" value="Hello World"  />
				<var static="true" name="arr2"  type="Array" />
				
				<function static="true" name="Main" >
					<OThis/>
					<OScope/>
					
					<OFindProp p="arr2"/><!--[class Main] -->
						<OString v="A" />
						<OSmallInt v="23" />
						<OFindPropStrict p="MovieClip"/>
						<OConstructProperty p="MovieClip" nargs="0"/>
						<OArray nargs="3"/>
					<OSetProp p="arr2" />
					
					<ORetVoid/>
				</function>
				
				<function name="Main" >
					<OThis/>
					<OScope/>
					
					<OThis/>
						<OString v="a" />
						<OSmallInt v="13" />
						<OFindPropStrict p="MovieClip"/>
						<OConstructProperty p="MovieClip" nargs="0"/>
						<OArray nargs="3"/>
					<OSetProp p="arr1" />

					<OThis/>
					<OConstructSuper nargs="0" />
					
					<OFindPropStrict p="trace"/>
						<OThis/><!--[object Main] -->
						<OGetProp p="str1" /><!--hello world-->
							
						<OFindProp p="str2"/><!--[class Main] -->
						<OGetProp p="str2" /><!--Hello World-->
						
						<OGetLex p="arr1" /><!--[object Main].arr1--><!--a,13,[object MovieClip]-->
						<OGetLex p="arr2" /><!--[class Main].arr2--><!--A,23,[object MovieClip]-->
					<OCallPropVoid p="trace" nargs="4" />

					<ORetVoid/>
				</function>
				
			</class>
			</abcfile>
		</abcfiles>
	</DefineABC>
	
	<SymbolClass id="0" class="Main"/>
	
	<ShowFrame/>
	
</swf>