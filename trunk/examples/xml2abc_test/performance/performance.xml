<swf>

	<Header/>
	
	<FileAttributes/>
	
	<DefineABC name="Main">
		<abcfiles>
			<abcfile>
			<import class="flash.display.MovieClip" />
			<import class="flash.utils.getTimer" />

			<class name="Main" extends="MovieClip">
				
				<function name="Main" >
					<OThis/><OScope/>
					<OThis/><OConstructSuper nargs="0" />

					<OFindPropStrict p="getTimer"/>
					<OCallProperty p="getTimer" nargs="0"/>
					<OSetReg r="1" />
					
					<OSmallInt v="0" />
					<OSetReg r="2"/>
					<JAlways jump="whileCond"/>

					<OLabel name="whileBody" />
					<!--
					testing code here
					-->
					<OIncrIReg r="2"/>

					<OJump name="whileCond"/>
					<OReg r="2" />
					<OIntRef v="100000000"/>
					<JLt label="whileBody"/>
					
					<OFindPropStrict p="trace"/>
					<OReg r="1"/>
					<OFindPropStrict p="getTimer"/>
					<OCallProperty p="getTimer" nargs="0"/>
					<OCallPropVoid p="trace" nargs="2"/>

					<ORetVoid/>
					</function>
			</class>
			</abcfile>
		</abcfiles>
	</DefineABC>
	
	<SymbolClass id="0" class="Main"/>
	
	<ShowFrame/>
	
</swf>