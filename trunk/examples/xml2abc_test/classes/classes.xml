<!--
package 
{
    import flash.display.MovieClip;

    public class Main extends MovieClip
    {
        public var test:Test;

        public function Main()
        {
            this.test = new Test();
            trace(this.test.nr1, Test.nr2, this.test.func1(), Test.func2(), Test.func2());
            return;
        }
    }


    public class Test extends Object
    {
        public var nr1:int = 1;
        public static var nr2:int = 2;

        public function Test()
        {
            return;
        }

        public function func1()
        {
            return "Hello";
        }

        public static function func2()
        {
            return "world";
        }

    }
}
-->
<swf>
	<Header/>
	<FileAttributes/>
	
	<DefineABC name="Main">
		<abcfiles>
			<abcfile>
			<import class="flash.display.MovieClip" />

			<class name="Main" extends="MovieClip">
				
				<var name="test"  type="Test" />
				
				<function name="Main" >
					<OThis/><!--[object Main] -->
					<OScope/>
					
					<OThis/><!--[object Main] -->
					<OConstructSuper nargs="0" />

					<OThis/><!--[object Main] -->
						<OFindPropStrict p="Test"/><!--[object global] -->
						<OConstructProperty p="Test" nargs="0" /><!--[object Test] -->
					<OInitProp p="test" />

					<OFindPropStrict p="trace"/><!--[object global] -->				
						<OThis/><!--[object Main] -->
						<OGetProp p="test"/><!--[object Test] -->
						<OGetProp p="nr1"/><!--1 -->
						
						<OGetLex p="Test" /><!--[class Test] -->
						<OGetProp p="nr2"/><!--2 -->

						<OThis/><!--[object Main] -->		
						<OGetProp p="test"/><!--[object Test] -->
						<OCallProperty p="func1" nargs="0"/><!--Hello -->

						<OGetLex p="Test" /><!--[class Test] -->
						<OCallProperty p="func2" nargs="0"/><!--world-->
						
						<OFindPropStrict p="Test"/><!--[object global] -->
						<OGetProp p="Test" /><!--[class Test] -->
						<OCallProperty p="func2" nargs="0"/><!--world-->
						
					<OCallPropVoid p="trace" nargs="5" />
						
					<ORetVoid/>
				</function>
			</class>
			
			<class name="Test">
				<var name="nr1"  type="int" value="1" />
				<var static="true" name="nr2"  type="int" value="2" />
				
				<function name="Test" >
					<OThis/>	
					<OScope/>
					<OThis/><!--[object Test] -->	
					<OConstructSuper nargs="0" />
					<ORetVoid/>
					</function>
				
				<function name="func1">
					<OThis/>	
					<OScope/>
					<OString v="Hello" />
					<ORet/>
					</function>
				
				<function static="true" name="func2" >
					<OThis/>
					<OScope/>
					<OString v="world" />
					<ORet/>
					</function>
				
			</class>
		</abcfile>
		</abcfiles>
	</DefineABC>
	
	<SymbolClass id="0" class="Main"/>
	
	<ShowFrame/>
	
</swf>