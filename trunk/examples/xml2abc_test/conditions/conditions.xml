<swf>

	<Header/>
	
	<FileAttributes/>
	
	<DefineABC name="Main">
		<abcfiles>
			<abcfile>
			<import class="flash.display.MovieClip" />

			<class name="Main" extends="MovieClip">
				
				<var name="nr1"  type="int" value="2" />
				
				<function name="Main" >
					<OThis/><OScope/>
					<OThis/><OConstructSuper nargs="0" />

					<OThis/>
					<OGetProp p="nr1"/>
					<OSmallInt v="1" />
					<JNeq jump="neq1" />
						<OFindPropStrict p="trace" />
						<OString v="one"/>
						<OCallPropVoid p="trace" nargs="1" />
						<JAlways jump="always1" />
					
					<OJump name="neq1" />
					<OThis/>
					<OGetProp p="nr1"/>
					<OSmallInt v="2" />
					<JNeq jump="neq2"/>
							<OFindPropStrict p="trace" />
							<OString v="two"/>
							<OCallPropVoid p="trace" nargs="1" />
							<JAlways jump="always2" />
					
					<OJump name="neq2"/>
					<OThis/>
					<OGetProp p="nr1"/>
					<OSmallInt v="3" />
					<JNeq jump="neq3"/>
						<OFindPropStrict p="trace" />
						<OString v="three"/>
						<OCallPropVoid p="trace" nargs="1" />
						<JAlways jump="always3" />
					
					<OJump name="neq3"/>
					<OFindPropStrict p="trace" />
					<OString v="other"/>
					<OCallPropVoid p="trace" nargs="1" />
					
					<OJump name="always1"/>
					<OJump name="always2"/>
					<OJump name="always3"/>
					
					
					<OSmallInt v="0" />
					<OSetReg r="1"/>
					<JAlways jump="whileCond"/>

					<OLabel name="whileBody" />
					<OFindPropStrict p="trace" />
					<OReg r="1"/>
					<OCallPropVoid p="trace" nargs="1" />
					<OIncrIReg r="1"/>
					
					<OJump name="whileCond"/>
					<OReg r="1" />
					<OSmallInt v="5"/>
					<JLt label="whileBody"/>
					
					<ORetVoid/>
					</function>
			</class>
			</abcfile>
		</abcfiles>
	</DefineABC>
	
	<SymbolClass id="0" class="Main"/>
	
	<ShowFrame/>
	
</swf>
