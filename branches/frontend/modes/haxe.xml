<?xml version="1.0" encoding="iso-8859-1"?><rules>		<rule name="comment" value="\/\/.*" color="0x999999"/>		<rule name="class" value="new (\w+)" color="0x007777"/>		<rule name="function" value="\.(\w+)" color="0x430044"/>		<rule name="variable" value="var (\w+)" color="0x430044"/>				<rule name="keyword1" value="(import|class|extends)" color="0x0000FF"/>		<rule name="keyword2" value="(Std|Int|Float|Bool)" color="0x00CCFF"/>		<rule name="keyword3" value="(var|public|private|static|inline|function)" color="0x228800"/>		<rule name="keyword4" value="(new|cast|untyped)" color="0x223366"/>		<rule name="digit" value="\d" color="0x226611"/>		<rule name="operator" value="(+|-|*|/|^)" color="0x202020"/>		<rule name="string" value='".*"' color="0x882276"/>		<rule name="regex" value='~\/".*\/' color="0x335540"/></rules>