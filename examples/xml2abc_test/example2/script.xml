<!--
package 
{
    import flash.display.*;
    import flash.events.*;

    public class Main extends MovieClip
    {
        public var mc:Object;

        public function Main()
        {
            mc.buttonMode = true;
            mc.addEventListener(MouseEvent.CLICK, onClick);
            return;
        }

        public function onClick(param1:MouseEvent)
        {
            param1.currentTarget.x = param1.currentTarget.x + 10;
            param1.currentTarget.y = param1.currentTarget.y + 10;
            return;
        }

    }
}
-->
<abcfiles>
<abcfile>
			<class name="Main" extends="flash.display.MovieClip" >

				<var name="mc"/>

				<function name="Main">
					<OThis/>
					<OScope/>
					
					<OThis/>
					<OConstructSuper nargs="0" />
					
					<OGetLex p="mc"/>
						<OTrue/>
					<OSetProp p="buttonMode"/>
					
					<OGetLex p="mc" />
						<OGetLex p="flash.events.MouseEvent"/><OGetProp p="CLICK"/>
						<OGetLex p="onClick"/>
					<OCallPropVoid name="addEventListener" nargs="2"/>
					
					<ORetVoid/>
					
				</function>
				

				<function name="onClick" args="flash.events.MouseEvent">
					<OThis/>
					<OScope/>
					
					<OReg r="1"/><OGetProp p="currentTarget"/>
						<OReg r="1"/><OGetProp p="currentTarget"/><OGetProp p="x"/><OSmallInt v="10" /><OpAdd/>
					<OSetProp p="x" />
					
					<OReg r="1"/><OGetProp p="currentTarget"/>
						<OReg r="1"/><OGetProp p="currentTarget"/><OGetProp p="y"/><OSmallInt v="10" /><OpAdd/>
					<OSetProp p="y" />
					
					<ORetVoid/>
				</function>
				
			</class>
		</abcfile>
</abcfiles>